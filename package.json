{
  "name": "ts-code-layout",
  "version": "1.0.12",
  "description": "Changes the layout of typescript files.",
  "main": "main.js",
  "type": "module",
  "author": {
    "name": ""
  },
  "bin": {
    "ts-code-layout": "main.js"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "tsc --build --clean"
  },
  "files": [
    "ts-code-layout.json"
  ],
  "devDependencies": {
    "@types/node": "^14.18.54",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.47.0",
    "typescript": "^4.9.5"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint"
    ]
  },
  "dependencies": {
    "ts-morph": "^19.0.0"
  }
}
